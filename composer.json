{
    "name": "born-mt/mita-gpg-sdk",
    "description": "PHP SDK for Malta Government Payment Gateway (GPG) API integration",
    "keywords": [
        "malta",
        "gpg",
        "government-payment-gateway",
        "mita",
        "payment",
        "apcopay",
        "3d-secure"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Born MT",
            "email": "info@bornmt.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "guzzlehttp/guzzle": "^7.5",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "BornMT\\MitaGpg\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BornMT\\MitaGpg\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit -c phpunit.xml.dist",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}